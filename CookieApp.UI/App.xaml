<Application x:Class="CookieApp.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CookieApp.UI"
             xmlns:viewModels="clr-namespace:CookieApp.UI.ViewModels"
             xmlns:views="clr-namespace:CookieApp.UI.Views"
             Startup="OnStartUp">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewModels:TroopViewModel}">
            <views:TroopView/>
        </DataTemplate>
    </Application.Resources>
</Application>
