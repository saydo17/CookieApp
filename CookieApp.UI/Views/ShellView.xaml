<Window x:Class="CookieApp.UI.Views.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CookieApp.UI.Views"
        xmlns:viewModels="clr-namespace:CookieApp.UI.ViewModels"
        mc:Ignorable="d"
        Title="ShellViewModel" Height="750" Width="1050"
        d:DataContext="{d:DesignInstance Type={x:Type viewModels:ShellViewModel}}">
    <Grid>
        <ContentControl Content="{Binding CurrentViewModel}"/>
    </Grid>
</Window>
